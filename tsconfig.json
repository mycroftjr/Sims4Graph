{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2021",
    "lib": ["es2021"],
    "outDir": "out",
    "sourceMap": true,
    "strict": true,
    "rootDir": "src",
    "baseUrl": "./src",
    "moduleResolution": "node",
    "paths": {
      //"#constants": ["constants.ts"],
      //"#building/*": ["core/building/*"],
    },
    "esModuleInterop": true,
  },
  "include": ["src/**/*.ts"],
  "exclude": ["node_modules", ".vscode-test"],
}
